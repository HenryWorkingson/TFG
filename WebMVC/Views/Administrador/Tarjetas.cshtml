@model IEnumerable<Restaurante.Tarjeta>

@{
    ViewData["Title"] = "Tarjetas";
}

<h1>Tarjetas</h1>


<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.id_Tarjeta)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Numero_Tarjeta)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FechaCadu_Tarjeta)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Nom_Propietario)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.id_Tarjeta)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Numero_Tarjeta)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FechaCadu_Tarjeta)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Nom_Propietario)
            </td>
            
        </tr>
}
    </tbody>
</table>
